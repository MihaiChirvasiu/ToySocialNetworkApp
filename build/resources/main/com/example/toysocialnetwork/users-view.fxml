   <children>
      <TableView fx:id="tableView" layoutY="8.0" prefHeight="200.0" prefWidth="335.0">
        <columns>
          <TableColumn fx:id="tableColumnFirstName" prefWidth="150.0" text="FirstName" />
          <TableColumn fx:id="tableColumnLastName" prefWidth="150.0" text="LastName" />
        </columns>
      </TableView>
      <Button layoutY="208.0" mnemonicParsing="false" onAction="#handleFriendRequests" text="ShowDetails" />
   </children>
</AnchorPane>
